{
  "name": "triz-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --mode production",
    "build:docker": "tsc && vite build --mode docker",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint:fix": "eslint . --fix && prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "generate": "plop",
    "docker:build": "tsc && docker build -t triz-client .",
    "docker:run": "docker rm -f triz-client && docker run --name triz-client -d -p 80:8082 triz-client",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ci": "vitest run --coverage --bail=1",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "interface": "i18next-resources-for-ts interface -i ./public/locales/en -o ./src/configs/i18next/resources.d.ts"
  },
  "dependencies": {
